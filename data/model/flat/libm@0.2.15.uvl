features
    "libm"
        optional
            "unstable-public-internals"
            "unstable-intrinsics"
                optional
                    "unstable"
            "unstable-float"
            "force-soft-floats"
            "default"
            "arch"
constraints
    "default" => "arch"
    "unstable" => "unstable-float" & "libm"
