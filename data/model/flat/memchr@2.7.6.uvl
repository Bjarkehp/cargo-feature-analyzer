features
    "memchr"
        optional
            "log"
                optional
                    "logging"
            "core"
                optional
                    "rustc-dep-of-std"
            "use_std"
            "std"
                optional
                    "default"
            "libc"
            "alloc"
constraints
    "logging" => "memchr"
    "rustc-dep-of-std" => "memchr"
    "use_std" => "std"
    "std" => "alloc"
    "default" => "memchr"
