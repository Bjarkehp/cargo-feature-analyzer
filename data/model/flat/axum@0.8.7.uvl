features
    "axum"
        optional
            "tower-http"
                optional
                    "__private_docs"
            "tokio-tungstenite"
                optional
                    "ws"
            "sha1"
            "serde_urlencoded"
                optional
                    "query"
                        optional
                            "default"
                    "form"
            "serde_path_to_error"
                optional
                    "json"
            "serde_json"
            "serde"
            "reqwest"
                optional
                    "__private"
            "multer"
                optional
                    "multipart"
            "hyper-util"
                optional
                    "tokio"
            "hyper"
                optional
                    "http2"
                    "http1"
            "form_urlencoded"
            "base64"
            "axum-macros"
                optional
                    "macros"
            "tracing"
            "tower-log"
            "original-uri"
            "matched-path"
constraints
    "tokio" => "axum"
    "http1" => "axum"
    "query" => "serde_path_to_error" & "form_urlencoded" & "axum"
    "ws" => "sha1" & "tokio" & "hyper" & "base64" & "axum"
    "form" => "serde_path_to_error" & "form_urlencoded" & "axum"
    "macros" => "axum"
    "__private_docs" => "serde" & "axum"
    "__private" => "tokio" & "http1" & "axum"
    "default" => "form" & "json" & "tokio" & "http1" & "tracing" & "tower-log" & "original-uri" & "matched-path" & "axum"
    "json" => "serde_json" & "axum"
    "multipart" => "axum"
    "http2" => "axum"
