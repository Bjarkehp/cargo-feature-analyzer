features
    "tonic"
        optional
            "webpki-roots"
                optional
                    "tls-webpki-roots"
            "tower"
                optional
                    "server"
                        optional
                            "transport"
                                optional
                                    "default"
                    "router"
                    "channel"
                        optional
                            "tls-native-roots"
            "tokio-rustls"
                optional
                    "tls-ring"
                    "tls-connect-info"
                        optional
                            "_tls-any"
                                optional
                                    "tls-aws-lc"
            "tokio"
            "socket2"
            "rustls-native-certs"
            "hyper-util"
            "hyper-timeout"
            "hyper"
            "h2"
            "flate2"
                optional
                    "gzip"
                    "deflate"
            "axum"
            "async-trait"
                optional
                    "codegen"
            "zstd"
constraints
    "default" => "router" & "codegen" & "tonic"
    "tls-ring" => "_tls-any" & "tonic"
    "deflate" => "tonic"
    "tls-aws-lc" => "tokio-rustls" & "tonic"
    "_tls-any" => "tokio" & "tonic"
    "tls-connect-info" => "tonic"
    "codegen" => "tonic"
    "router" => "axum" & "tonic"
    "tls-native-roots" => "_tls-any" & "rustls-native-certs" & "tonic"
    "tls-webpki-roots" => "channel" & "_tls-any" & "tonic"
    "transport" => "channel" & "tonic"
    "server" => "tokio" & "socket2" & "hyper-util" & "hyper" & "h2" & "tonic"
    "channel" => "tokio" & "hyper-util" & "hyper-timeout" & "hyper" & "tonic"
    "gzip" => "tonic"
