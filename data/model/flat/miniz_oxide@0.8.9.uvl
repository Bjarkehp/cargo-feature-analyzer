features
    "miniz_oxide"
        optional
            "simd-adler32"
                optional
                    "simd"
            "serde"
            "core"
                optional
                    "rustc-dep-of-std"
            "alloc"
            "with-alloc"
                optional
                    "default"
            "std"
            "block-boundary"
constraints
    "default" => "miniz_oxide"
    "simd" => "miniz_oxide"
    "rustc-dep-of-std" => "alloc" & "miniz_oxide"
