    "usage" => "clap_builder"
    "default" => "error_context" & "usage" & "help" & "std" & "clap" & "color"
    "unstable_markdown" => "clap_derive"
    "suggestions" => "clap_builder"
    "deprecated" => "clap_builder"
    "color" => "clap_builder"
    "unicode" => "clap_builder"
    "unstable_doc" => "derive" & "clap_builder"
    "help" => "clap_builder"
    "unstable_v5" => "deprecated" & "clap_builder"
    "unstable_ext" => "clap_builder"
    "unstable_styles" => "clap_builder"
    "cargo" => "clap"
    "std" => "clap_builder"
    "error_context" => "clap_builder"
