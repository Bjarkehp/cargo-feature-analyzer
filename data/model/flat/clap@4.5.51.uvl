features
    "clap"
        optional
            "wrap_help"
            "unstable_v5"
            "unstable_styles"
            "unstable_markdown"
            "unstable_ext"
            "unstable_doc"
            "unstable_derive_ui_tests"
            "unicode"
            "string"
            "env"
            "clap_derive"
                optional
                    "derive"
            "deprecated"
            "suggestions"
                optional
                    "default"
            "error_context"
            "usage"
            "help"
            "std"
            "debug"
            "color"
            "clap_builder"
