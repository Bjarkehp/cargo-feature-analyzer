features
    "uuid"
        optional
            "rand"
                optional
                    "rng-rand"
                    "fast-rng"
            "getrandom"
                optional
                    "rng-getrandom"
                    "rng"
                        optional
                            "v7"
                            "v4"
            "js-sys"
                optional
                    "js"
            "wasm-bindgen"
            "uuid-rng-internal-lib"
            "zerocopy"
            "uuid-macro-internal"
                optional
                    "macro-diagnostics"
            "slog"
            "sha1_smol"
                optional
                    "sha1"
                        optional
                            "v5"
            "serde"
            "md-5"
                optional
                    "md5"
                        optional
                            "v3"
            "bytemuck"
            "borsh-derive"
                optional
                    "borsh"
            "arbitrary"
            "v8"
            "v6"
            "v1"
            "std"
                optional
                    "default"
            "atomic"
constraints
    "v3" => "uuid"
    "v5" => "uuid"
    "v7" => "uuid"
    "js" => "wasm-bindgen" & "uuid"
    "rng" => "uuid"
    "macro-diagnostics" => "uuid"
    "default" => "uuid"
    "md5" => "uuid"
    "v1" => "atomic"
    "v6" => "atomic"
    "rng-rand" => "rng" & "uuid-rng-internal-lib" & "uuid"
    "v4" => "uuid"
    "borsh" => "uuid"
    "fast-rng" => "rng" & "uuid"
    "sha1" => "uuid"
    "rng-getrandom" => "rng" & "uuid-rng-internal-lib" & "uuid"
