features
	"hashbrown"
		optional
			"alloc"
			"inline-more"
				optional
					"default-hasher"
						mandatory
							"foldhash"
						optional
							"serde"
							"default"
					"allocator-api2"
			"equivalent"
				mandatory
					"raw-entry"
		optional // Unused features
			"core"
			"nightly"
			"rayon"
			"rustc-dep-of-std"
			"rustc-internal-api"
constraints
	"alloc" => !"default" & !"serde"
	"default" => !"alloc" & "allocator-api2" & "equivalent" & "raw-entry"
	"serde" => !"alloc"
