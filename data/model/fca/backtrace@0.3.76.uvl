features
	"backtrace"
		optional
			"std"
				or
					"serde"
					"default"
						optional
							"cpp_demangle"
		optional // Unused features
			"coresymbolication"
			"dbghelp"
			"dl_iterate_phdr"
			"dladdr"
			"kernel32"
			"libunwind"
			"ruzstd"
			"serialize-serde"
			"unix-backtrace"
constraints
	"cpp_demangle" => !"serde"
	"serde" => !"cpp_demangle"
