features
	"serde_json"
		optional
			"alloc"
			"default"
				mandatory
					"std"
				optional
					"unbounded_depth"
					"indexmap"
						mandatory
							"preserve_order"
		optional // Unused features
			"arbitrary_precision"
			"float_roundtrip"
			"raw_value"
constraints
	"alloc" => !"indexmap" & !"unbounded_depth"
	"indexmap" => !"alloc" & !"unbounded_depth"
	"unbounded_depth" => !"alloc" & !"indexmap"
