features
	"serde_json"
		alternative
			"alloc"
			"default"
				mandatory
					"std"
				optional
					"indexmap"
						mandatory
							"preserve_order"
		optional // Unused features
			"arbitrary_precision"
			"float_roundtrip"
			"raw_value"
			"unbounded_depth"
constraints
	"alloc" => !"indexmap"
	"indexmap" => !"alloc"
