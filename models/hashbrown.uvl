features
	"hashbrown"
		optional
			"alloc"
			"rayon"
			"serde"
			"equivalent"
				mandatory
					"raw-entry"
				optional
					"default"
			"inline-more"
				optional
					"allocator-api2"
			"default-hasher"
				mandatory
					"foldhash"
		optional // Unused features
			"nightly"
			"bumpalo"
			"rustc-dep-of-std"
			"core"
			"rustc-internal-api"
constraints
	"alloc" => !"default" & !"rayon" & !"serde"
	"default" => !"alloc" & "allocator-api2" & "default-hasher" & "foldhash"
	"rayon" => !"alloc" & !"serde"
	"serde" => !"alloc" & !"rayon"
